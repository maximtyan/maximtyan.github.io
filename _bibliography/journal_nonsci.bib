@article{__2025,
	title = {하이브리드 전기 항공기 사이징에서의 전력-에너지 균형 요구 : 유상하중과 기술적 요소의 고려},
	volume = {53},
	issn = {1225-1348},
	url = {http://www.dbpia.co.kr/journal/articleDetail?nodeId=NODE12118690},
	abstract = {최근 항공 산업에서 지속 가능성이 강조되는 가운데, 전기추진 항공기의 도입이 가속화되고 있으며, 이에 전기추진 시스템에 적합한 사이징 방법이 요구되고 있다. 본 연구에서는 항공기 임무 형상에 따라 배터리의 요구 전력과 동력을 균형적으로 분배함으로써, 기존의 단일 동력 분담률을 사용하는 방법들이 가졌던 과대 추정 문제를 해결하는 사이징 방법론을 제시하였다. 제시된 방법론들은 검증을 통해 약 10\%의 오차율을 보이며 그 타당성을 입증하였다. 또한, 선행 연구로부터 도출된 Part 23급 수소연료전지 항공기와의 비교를 통해 하이브리드 시스템의 효용성을 확인하고자, 본 연구에서 제시한 사이징 방법론을 활용하여 미래 기술 수준에 따른 하이브리드 추진 시스템의 Case Study를 수행하였다. 나아가 인증 기준에서 제시하는 요구사항을 만족하기 위한 유상하중 Tradeoff를 수행한 결과, 동일 최대이륙중량 10,343kg에 대해 하이브리드 시스템에서는 약 255kg의 유상하중을 더 수용하며, 최대이륙중량 인증요건 8,618kg에 대해서는 115kg의 유상 하중 손실 감소를 확인하였다.},
	language = {English},
	number = {4},
	journal = {한국항공우주학회지},
	author = {{최용현} and {Zin Win Thu} and {Maxim Tyan} and {정우석} and {이재우}},
	month = apr,
	year = {2025},
	note = {Publisher: 한국항공우주학회},
	keywords = {Electric Propulsion Aircraft(전기 추진 항공기), Hybrid Aircraft(하이브리드 항공기), Hybridization(하이브리드화), Initial Sizing(초기 사이징)},
	pages = {421--432},
	file = {PDF:C\:\\Users\\m.tyan\\Zotero\\storage\\7MD728BQ\\최용현 et al. - 2025 - 하이브리드 전기 항공기 사이징에서의 전력-에너지 균형 요구  유상하중과 기술적 요소의 고려.pdf:application/pdf},
}

@article{lv_thang_nguyen_comprehensive_2025,
	title = {Comprehensive {Modeling} of {Electric} {Propulsion} {System} for {High}-fidelity {eVTOL}-{AAM} {Flight} {Simulation}},
	volume = {53},
	issn = {1225-1348},
	url = {http://www.dbpia.co.kr/journal/articleDetail?nodeId=NODE12087088},
	abstract = {최근 몇 년간, 미래 항공 모빌리티(Advanced Air Mobility, AAM)은 높은 인구 밀도의 도심 지녁을 위한 혁신적인 해결책으로 떠오르고 있으며, 일반적으로 전기 추진 수직 이착륙 (eVTOL) 항공기의 형태를 가진다. 이러한 eVTOL 항공기에 대한 수요가 증가함에 따라, 고정밀 비행 시뮬레이션의 필요성도 증가하고 있다. 이때, 전기 추진 시스템은 eVTOL 항공기의 비행 역학에서 핵심적인 요소 중 하나이다. 이에 본 연구에서는 eVTOL 항공기의 비행 시뮬레이션을 위한 전기 추진 시스템의 종합적인 모델을 개발하는 것을 목표로 한다. 이때, 이 모델은 전기 추진 시스템 구성 요소에 대한 상세한 수학적 표현과 항공기 비행 역학과의 상호작용을 포함한다. 전체 통합 시스템의 신뢰성은 축소형 프로토타입의 UAM 항공기를 대상으로 하여 지상 및 비행 시험 시나리오 사례 연구를 통행 평가된다. 시뮬레이션 결과와 실험 데이터 간의 일치도는 모델이 강건함을 입증하며, 향후 가상 비행 인증 기술 및 AAM 분야의 디지털 트윈 기술을 발전시키는데 크게 기여할 수 있다.},
	language = {English},
	number = {3},
	journal = {한국항공우주학회지},
	author = {{LV Thang Nguyen} and {Donggyu Seong} and {Mohammad Irfan Alam} and {Maxim Tyan} and {Jae-Woo Lee}},
	month = mar,
	year = {2025},
	note = {Publisher: 한국항공우주학회},
	keywords = {Advanced Air Mobility(미래항공모빌리티), Electric Propulsion System(전기추진시스템), eVTOL aircraft(전기동력 수직이착륙 항공기), Flight Simulation(비행 시뮬레이션)},
	pages = {239--248},
	file = {PDF:C\:\\Users\\m.tyan\\Zotero\\storage\\KRSFI4RE\\LV Thang Nguyen et al. - 2025 - Comprehensive Modeling of Electric Propulsion System for High-fidelity eVTOL-AAM Flight Simulation.pdf:application/pdf},
